1.pkg up -y;pkg install git -y;pkg install wget -y;pkg install curl -y
#64 BIT
2. git clone https://github.com/White9shadow/no-root-ram-booster

3. cd no-root-ram-booster

4. chmod +x GSNRRM64

5. ./GSNRRM64

